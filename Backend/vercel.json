{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server.js"
    }
  ],
  "env": {
    "OPENAI_API_KEY": "@openai_api_key",
    "PORT": "3001",
    "FRONTEND_URL": "@frontend_url",
    "NODE_ENV": "production"
  }
}
